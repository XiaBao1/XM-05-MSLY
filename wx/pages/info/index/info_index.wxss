/**index.wxss**/
@import "../../template/template.wxss";

.content-below{
  width: 100%;
  height: 600rpx;
  margin-top: 0rpx;
  position: relative;
}
.icon-list{
  width: 94%;
  height: 450rpx;
  margin: auto;
  background: #ffffff;
  position: absolute;
  top: 20rpx;
  left:0;
  right: 0;
  border-radius: 8rpx;
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
}
.icon-text{
  width: 33%;
  height: 78rpx;
  margin-top: 72rpx;
}
.icon{
  width: 80rpx;
  height: 80rpx;
  display: block;
  margin:auto;
}
.text{
  text-align: center;
  font-size: 26rpx;
  line-height: 26rpx;
  margin-top: 16rpx;
  color:#333333;
}


swiper{
  height: 400rpx;
}
page{
  width: 100%;
  background: #ededed;
}
.banner{
  width: 100%;
  height: 400rpx;
  display: block;
  margin-top: 16rpx;
}