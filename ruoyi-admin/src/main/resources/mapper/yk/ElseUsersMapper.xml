<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.yk.mapper.ElseUsersMapper">
    
    <resultMap type="ElseUsers" id="ElseUsersResult">
        <result property="userNumber"    column="user_number"    />
        <result property="password"    column="password"    />
        <result property="username"    column="username"    />
        <result property="gender"    column="gender"    />
        <result property="registerTime"    column="register_time"    />
        <result property="province"    column="province"    />
        <result property="email"    column="email"    />
        <result property="money"    column="money"    />
        <result property="photo"    column="photo"    />
        <result property="phone"    column="phone"    />
        <result property="roleId"    column="role_id"    />
    </resultMap>

    <sql id="selectElseUsersVo">
        select user_number, password, username, gender, register_time, province, email, money, photo, phone, role_id from else_users
    </sql>

    <select id="selectElseUsersList" parameterType="ElseUsers" resultMap="ElseUsersResult">
        <include refid="selectElseUsersVo"/>
        <where>  
            <if test="password != null  and password != ''"> and password = #{password}</if>
            <if test="username != null  and username != ''"> and username like concat('%', #{username}, '%')</if>
            <if test="gender != null  and gender != ''"> and gender = #{gender}</if>
            <if test="registerTime != null "> and register_time = #{registerTime}</if>
            <if test="province != null  and province != ''"> and province = #{province}</if>
            <if test="email != null  and email != ''"> and email = #{email}</if>
            <if test="money != null "> and money = #{money}</if>
            <if test="photo != null  and photo != ''"> and photo = #{photo}</if>
            <if test="phone != null  and phone != ''"> and phone = #{phone}</if>
            <if test="roleId != null "> and role_id = #{roleId}</if>
        </where>
    </select>
    
    <select id="selectElseUsersByUserNumber" parameterType="String" resultMap="ElseUsersResult">
        <include refid="selectElseUsersVo"/>
        where user_number = #{userNumber}
    </select>
        
    <insert id="insertElseUsers" parameterType="ElseUsers">
        insert into else_users
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userNumber != null">user_number,</if>
            <if test="password != null and password != ''">password,</if>
            <if test="username != null and username != ''">username,</if>
            <if test="gender != null and gender != ''">gender,</if>
            <if test="registerTime != null">register_time,</if>
            <if test="province != null">province,</if>
            <if test="email != null">email,</if>
            <if test="money != null">money,</if>
            <if test="photo != null">photo,</if>
            <if test="phone != null">phone,</if>
            <if test="roleId != null">role_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userNumber != null">#{userNumber},</if>
            <if test="password != null and password != ''">#{password},</if>
            <if test="username != null and username != ''">#{username},</if>
            <if test="gender != null and gender != ''">#{gender},</if>
            <if test="registerTime != null">#{registerTime},</if>
            <if test="province != null">#{province},</if>
            <if test="email != null">#{email},</if>
            <if test="money != null">#{money},</if>
            <if test="photo != null">#{photo},</if>
            <if test="phone != null">#{phone},</if>
            <if test="roleId != null">#{roleId},</if>
         </trim>
    </insert>

    <update id="updateElseUsers" parameterType="ElseUsers">
        update else_users
        <trim prefix="SET" suffixOverrides=",">
            <if test="password != null and password != ''">password = #{password},</if>
            <if test="username != null and username != ''">username = #{username},</if>
            <if test="gender != null and gender != ''">gender = #{gender},</if>
            <if test="registerTime != null">register_time = #{registerTime},</if>
            <if test="province != null">province = #{province},</if>
            <if test="email != null">email = #{email},</if>
            <if test="money != null">money = #{money},</if>
            <if test="photo != null">photo = #{photo},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="roleId != null">role_id = #{roleId},</if>
        </trim>
        where user_number = #{userNumber}
    </update>

    <delete id="deleteElseUsersByUserNumber" parameterType="String">
        delete from else_users where user_number = #{userNumber}
    </delete>

    <delete id="deleteElseUsersByUserNumbers" parameterType="String">
        delete from else_users where user_number in 
        <foreach item="userNumber" collection="array" open="(" separator="," close=")">
            #{userNumber}
        </foreach>
    </delete>

</mapper>